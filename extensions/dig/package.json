{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dig",
  "title": "Dig (DNS Lookup)",
  "description": "**digs a host**\nex. dig facebook.com ns = returns nameserver for Facebook\nex. dig raycast.com mx = returns mailexchangers for raycast.com",
  "icon": "command-icon.png",
  "author": "danielbahl",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "dig",
      "subtitle": "digs a hostname for ns-records",
      "description": "Returns NS-records from the hostname",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.31.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
